{
  "manifest_version": 3,
  "name": "Zalo Content Masker",
  "version": "1.0",
  "description": "Ẩn nội dung các thẻ cụ thể trên Zalo Web",
  "permissions": ["scripting"],
  "content_scripts": [
    {
      "matches": ["*://*.zalo.me/*"],
      "js": ["content.js"],
      "css": ["style.css"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "48": "icon.png"
  }
}
